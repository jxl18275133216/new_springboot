<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jxl.shabby_for_me.user.dao.UserDao">
    <insert id="insertObject" parameterType="com.jxl.shabby_for_me.user.entity.User">
        insert user
        (grade,username,userpwd,createtime)
        values
        (#{grade},#{username},#{userpwd},now())
    </insert>
</mapper>